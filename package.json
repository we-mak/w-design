{
  "name": "w-design",
  "version": "0.8.0",
  "description": "A minimal React UI library build with styled-components",
  "author": "Sean",
  "license": "MIT",
  "repository": "we-mak/w-design",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 jest test ./src --env=jsdom",
    "test:watch": "jest test ./src --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "clean-tag": "^2.0.2",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "@svgr/rollup": "^4.1.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.1.4",
    "@types/styled-system": "^3.0.9",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.1.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-scripts": "^2.1.2",
    "react-test-renderer": "^16.7.0",
    "rollup": "^0.68.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript3": "^1.1.2",
    "rollup-plugin-url": "^2.1.0",
    "styled-components": "^4.1.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
