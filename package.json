{
  "name": "w-design",
  "version": "0.37.4",
  "description": "A React UI library is built with css-in-js, typescript",
  "author": "viiiprock",
  "license": "MIT",
  "repository": "we-mak/w-design",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 jest test ./src --env=jsdom -u --colors --verbose",
    "test:watch": "jest -u --colors --verbose",
    "build": "cross-env NODE_ENV=production rollup -c",
    "start": "rollup -c -w",
    "coverage": "jest --coverage -u",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "prepare": "yarn run build",
    "predeploy": "yarn install && yarn run build",
    "deploy": "gh-pages -d ./example/build"
  },
  "keywords": [
    "react",
    "styled-system",
    "UI library",
    "typescript",
    "minimal"
  ],
  "dependencies": {
    "@types/body-scroll-lock": "^2.6.1",
    "@types/styled-system": "^5.0.0",
    "body-scroll-lock": "^2.6.4",
    "styled-system": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "@svgr/rollup": "^4.1.0",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.1.3",
    "@testing-library/react-hooks": "^3.1.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/styled-components": "^4.1.16",
    "babel-plugin-styled-components": "^1.10.1",
    "coveralls": "^3.0.2",
    "cross-env": "^6.0.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^7.0.0-beta.1",
    "jest-transform-css": "^2.0.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-is": "^16.8.6",
    "react-test-renderer": "^16.9.0",
    "rollup": "^1.23.1",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-url": "^3.0.0",
    "styled-components": "^5.0.0-rc.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "styled-components": "^5.0.0-rc.0"
  },
  "files": [
    "dist"
  ]
}
